algoritmo "Exercicio28"
var
   N1, N2 ,N3, S: Real
inicio
      Escreva("Digite um numero: ")
      Leia (N1)
      Escreva("Digite outro numero: ")
      Leia (N2)
      Escreva("Digite mais um numero: ")
      Leia (N3)
      S <- (N1 + N2 + N3) ^ 2
      Escreva("O resultado da soma dos quadrados dos tres numeros sera: ", S)
fimalgoritmo
