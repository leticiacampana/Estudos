algoritmo "Exercicio29"
var
   N1, N2, N3, N4: Real
   M: Real
inicio
      Escreva("Digite um numero: ")
      Leia (N1)
      Escreva("Digite outro numero: ")
      Leia (N2)
      Escreva("Digite mais um numero: ")
      Leia (N3)
      Escreva("Digite um numero: ")
      Leia (N4)
      M <- (N1 + N2 + N3 + N4) / 4
      Escreval("A media  e: ", M)
fimalgoritmo
