algoritmo "contarAteQuanto"
var
   Valor, contador: Inteiro
inicio
      contador <- 0
      Escreva("Quer contar ate quanto? ")
      Leia (Valor)
      Enquanto (contador <=  Valor) faca
               Escreval(Contador)
               contador <- contador + 1
      FimEnquanto
      Escreval("Terminei de contar")
fimalgoritmo
