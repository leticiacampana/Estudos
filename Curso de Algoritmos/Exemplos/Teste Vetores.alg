algoritmo "TesteVetores"
var
   V: Vetor[1..6] de Inteiro
   C: Inteiro
inicio
      Para c <- 1 ate 6 faca
           Escreva("Digite o ", c, "o. valor: ")
           Leia(V[c])
      FimPara
      
      Para c <- 1 ate 6 faca
           escreva ("{", v[c], "}")
      FimPara
fimalgoritmo
